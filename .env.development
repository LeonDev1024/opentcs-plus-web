# App
VITE_APP_TITLE=OpenTCS Plus
VITE_APP_ENV=development
VITE_APP_PORT=5173
VITE_APP_CONTEXT_PATH=/

# Backend API (proxied by Vite)
VITE_APP_BASE_API=/dev-api
VITE_APP_BASE_URL=/

# Security/crypto
VITE_APP_ENCRYPT=false
# Provide RSA keys only if you enable encryption
VITE_APP_RSA_PUBLIC_KEY=
VITE_APP_RSA_PRIVATE_KEY=

# OAuth / client id
VITE_APP_CLIENT_ID=plus-ui-pc

# Realtime features (optional)
VITE_APP_WEBSOCKET=false
VITE_APP_SSE=false

# External consoles (optional)
VITE_APP_MONITOR_ADMIN=
VITE_APP_SNAILJOB_ADMIN=
